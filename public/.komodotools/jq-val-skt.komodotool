  "name": "jq-val-skt", 
  "value": [
    "$.validator.addMethod('skt', function(value, element, parameter) {", 
    "   var v = value.toUpperCase();", 
    "   if (v != 'A' && v != 'B' && v != 'C' && v != 'D' && v != 'E' && v != 'F' && v != 'G' && v != 'H' && v != '') {", 
    "      v = false;", 
    "   } else {", 
    "      v = true;", 
    "   }", 
    "   return this.optional(element) || v;", 
    "}, 'Invalid SKT');!@#_currentPos!@#_anchor"
  ], 
